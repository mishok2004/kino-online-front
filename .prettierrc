{
	"trailingComma": "es5",
	"tabWidth": 2,
	"semi": false,
	"singleQuote": true,
	"useTabs": true,

	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^@/assets/(.*)$",
		"^@/components/(.*)$",
		"^@/screen(.*)$",
		"^@/ui(.*)$",
		"^@hooks/(.*)$",
		"^@services/(.*)$",
		"^@shared/(.*)$",
		"^@store/(.*)$",
		"^@utils/(.*)$",
		"^[../]",
		"^[./]",
		"(.scss)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
